import java.util.Scanner;

public class MenuOptions {
	
		boolen isPrime(int n) {
			int factor = 0;
			for(int i=1; i<= n; i++) {
				if(n%i == 0) {
					factor++;
				}
			}
			return factor==2?true:false;
		}
