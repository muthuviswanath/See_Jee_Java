
public class TwoD {
	public static void main(String[] args) {
		int[][] arr = {{1,2,3,4},
						{2,3,4,5}};
		System.out.println(arr.length);
		
		int myarr [][] = new int[5][4];
		int myarr_1 [][] = new int[6][];
		myarr_1[0] = new int[2];
		myarr_1[1] = new int[6];
		myarr_1[2] = new int[3];
		myarr_1[3] = new int[7];
		myarr_1[4] = new int[2];
		myarr_1[5] = new int[1];
		
		for(int i=0; i<myarr_1.length; i++) {
			for(int j=0; j<myarr_1[i].length;j++) {
				myarr_1[i][j] = (i+j)+2;
			}
		}
		
		for(int i=0; i<myarr_1.length; i++) {
			for(int j=0; j<myarr_1[i].length;j++) {
				System.out.print(myarr_1[i][j]);
			}
			System.out.println();
		}
	}
}
