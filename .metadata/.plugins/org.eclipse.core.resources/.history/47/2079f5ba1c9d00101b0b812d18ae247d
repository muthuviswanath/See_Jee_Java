import java.util.Scanner;

public class ArrayOps {

	public static void main(String[] args) {
		int[] a = {1,2,3};
		int ab[] = {1,2,3,4};
		int []b = {1,2,3,4}; // early initialization
		
		int []c = new int[5];
		Scanner s = new Scanner(System.in);
		for(int i=0; i<c.length; i++) {
		c[i] = s.nextInt();
		}
	}
}
