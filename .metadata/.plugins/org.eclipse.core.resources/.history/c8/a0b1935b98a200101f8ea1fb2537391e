package generics;

public class Alpha <T> {
    public Krish empid;
    public Gokul empName;
    public Muthu salary;
    public Nonsense department;

    Alpha(Krish empid){
        this.empid = empid;
    }

    void display(){
        System.out.println(this.empid);
    }
    
    Muthu show() {
    	return this.salary; 
    }
}
