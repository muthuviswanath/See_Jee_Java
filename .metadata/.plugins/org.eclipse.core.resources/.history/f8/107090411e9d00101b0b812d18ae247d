
public class SortedArray{
	public static void main(String[] args) {
		int a[] = {12,22,31,44,56,67};
		int b[] = {10,14,,23,33,45,48,49,58,62};
		int c[] = new int[a.length + b.length];
		
		for(int ele:a) {
			System.out.print(ele +" ");
		}
		System.out.println();
		for(int ele:b) {
			System.out.print(ele +" ");
		}
		int i=0, j=0, k = 0;
		while(i<a.length && j < b.length) {
			if(a[i]<b[j])
			c[k++] = a[i++];
			else
			c[k++] = b[j++];
		}
		while(i<a.length) {
			c[k++] = a[i++];
		}
		while(j < b.length) {
			c[k++] = b[j++];
		}
		System.out.println();
		for(int ele:c) {
			System.out.print(ele +" ");
		}
	}
}
